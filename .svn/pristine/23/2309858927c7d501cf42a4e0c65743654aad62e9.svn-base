@CHARSET "UTF-8";
.content-page {
	position:absolute;
	z-index: 1;
	border:1px solid #cecece;
	background-color: white;
	top: 50px;
	bottom: 5px;
	left: 5px;
	right:5px;
	box-shadow: 3px 3px 5px rgba(0,0,0,0.5);
	padding: 5px;
	overflow-x:hidden;
	overflow-y:auto;
	word-wrap:break-word;
	/*overflow:hidden;*/
}

#branch-info-box {
	z-index: 2;
	position:absolute;
	width:100%;
	top:46px;
	background-color:white;
	display:none;
	box-shadow:0px 2px 5px rgba(0,0,0,0.5);
	border-bottom: 1px solid #4f4f4f;
}